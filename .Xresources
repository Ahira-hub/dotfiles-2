*.color15: #c8c7c6
*color15:  #c8c7c6

! Black color that will not be affected by bold highlighting.
*.color66: #231f1d
*color66:  #231f1d

! Xclock colors.
XClock*foreground: #c8c7c6
XClock*background: #231f1d
XClock*majorColor:  rgba:c8/c7/c6/ff
XClock*minorColor:  rgba:c8/c7/c6/ff
XClock*hourColor:   rgba:c8/c7/c6/ff
XClock*minuteColor: rgba:c8/c7/c6/ff
XClock*secondColor: rgba:c8/c7/c6/ff

! Set depth to make transparency work.
URxvt*depth: 32
